{
  "name": "roznamcha-app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma-generate && npm run prisma-migrate && npm run next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma-generate",
    "prisma-generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma-migrate": "prisma migrate deploy --schema ./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@radix-ui/themes": "^2.0.3",
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "prisma": "^5.8.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
